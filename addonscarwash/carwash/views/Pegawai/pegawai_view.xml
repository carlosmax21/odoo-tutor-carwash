<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- carwash.pegawai tree view -->
    <record id="carwash_pegawai_view_tree" model="ir.ui.view">
        <field name="name">carwash.pegawai.view.tree</field>
        <field name="model">carwash.pegawai</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
                <field name="alamat" />
                <field name="tgl_lahir" />
                <field name="telp" />
            </tree>
        </field>
    </record>

    <!-- carwash.pegawai form view -->
    <record id="carwash_pegawai_view_form" model="ir.ui.view">
        <field name="name">carwash.pegawai.view.form</field>
        <field name="model">carwash.pegawai</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group name="data" string="Data">
                        <field name="name" />
                        <field name="alamat" />
                        <field name="tgl_lahir" />
                        <field name="telp" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

</odoo>
